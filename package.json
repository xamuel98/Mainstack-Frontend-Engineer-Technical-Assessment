        "@tanstack/react-query-devtools": "^5.90.2",
        "axios": "^1.12.2",
        "framer-motion": "^12.23.24",
        "lucide-react": "^0.511.0",
        "primeicons": "^7.0.0",
        "primereact": "^10.9.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-material-symbols": "^4.4.0",
        "react-router-dom": "^7.9.4",
        "recharts": "^3.3.0"
    },
    "devDependencies": {
        "@emotion/babel-plugin": "^11.13.5",
        "@eslint/js": "^9.25.0",
        "@testing-library/jest-dom": "^6.9.1",
