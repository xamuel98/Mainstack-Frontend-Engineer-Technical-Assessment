        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "lint-staged": "^16.2.4",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "sass": "^1.93.2",
        "typescript": "~5.8.3",
